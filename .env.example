# Voice Assistant Configuration
# Copy to .env and adjust for your environment

# Backend Configuration
BACKEND_PORT=3000

# n8n Integration
# Replace with your actual n8n webhook URL
N8N_WEBHOOK_URL=http://localhost:5678/webhook/voice-assistant

# Whisper API Configuration
WHISPER_API_URL=http://whisper-api:9000/inference
# Model options: tiny, base, small, medium, large
WHISPER_MODEL=small
# Language options: auto, en, de, fr, es, etc.
WHISPER_LANGUAGE=de

# Coqui TTS Configuration
COQUI_TTS_API_URL=http://coqui-tts-api:5002/api/tts
# Speaker WAV file to use for voice cloning
COQUI_SPEAKER_WAV=/app/speaker_files/Wj0v.wav
# Language for synthesis
COQUI_LANGUAGE=de
# Enable GPU acceleration
USE_CUDA=true

# Docker Configuration
COMPOSE_HTTP_TIMEOUT=300
